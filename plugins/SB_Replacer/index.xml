<?xml version="1.0" encoding="utf-8"?>
<plugin version="1.0">
  <title xml:lang="en">Inserter of Replacers</title>
  <title xml:lang="ko">태그 입력기</title>
  <link>http://needlworks.org/</link>
  <version>1.1</version>
  <description xml:lang="en">Insert replacers for plug-in's tag on sidebar.</description>
  <description xml:lang="ko">사이드바에 플러그인을 위한 치환자를 출력해 줍니다.</description>
  <license>GPL</license>
  <author xml:lang="en" link="http://gendoh.com">Sang-il Lee (gendoh / TNC)</author>
  <author xml:lang="ko" link="http://gendoh.com"><![CDATA[이상일 (gendoh / TNC)]]></author>
  <safety changeData="no" exposeData="no" accessLocal="no" accessRemote="no" accessRaw="no" />
  <requirements>
    <textcube>1.5</textcube>
  </requirements>
  <binding>
    <coverpage title="Replacer" handler="SB_Replacer">
      <params>
        <param>
          <title xml:lang="ko">HTML 코드</title>
          <title xml:lang="en">HTML Code</title>
          <name>text</name>
          <type>string</type>
        </param>
      </params>
    </coverpage>	
    <sidebar title="Replacer" handler="SB_Replacer">
      <params>
        <param>
          <title xml:lang="ko">HTML 코드</title>
          <title xml:lang="en">HTML Code</title>
          <name>text</name>
          <type>string</type>
        </param>
      </params>
    </sidebar>
  </binding>
</plugin>
